{
  "name": "internal-edhi",
  "version": "1.0.0",
  "description": "Edhi takes care of users.",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf ./dist",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run test:startdb; TEST_ENV=true TEST_STRIPE_LOCAL=${TEST_STRIPE_LOCAL:=true} LOG_LEVEL=${LOG_LEVEL:=silent} AWS_REGION=us-west-2 AWS_ACCESS_KEY_ID=AKID AWS_SECRET_ACCESS_KEY=SECRET LIGHTRAIL_DOMAIN=api.lightrail.local LIGHTRAIL_EMAIL_DOMAIN=lightrail.local LIGHTRAIL_WEBAPP_DOMAIN=lightrail.local OBJECT_TABLE=ObjectTable TOKEN_ACTION_TABLE=TokenActionTable mocha --recursive --throw-deprecation --require ts-node/register --require ./src/utils/testUtils/setLogLevel.ts --require ./src/utils/testUtils/dotEnvSafe.ts --require stripe-stateful-mock --exit \"src/**/*.test.ts\"; npm run test:stopdb",
    "test:debug": "LOG_LEVEL=debug npm run test",
    "test:startdb": "docker start edhi-test || (docker run --name edhi-test -p 8008:8000 amazon/dynamodb-local)",
    "test:stopdb": "docker stop edhi-test",
    "test:stripeLive": "TEST_STRIPE_LOCAL=false npm run test",
    "test:stripeLiveDebug": "TEST_STRIPE_LOCAL=false npm run test:debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giftbit/sam-scaffold.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Giftbit/internal-edhi/issues"
  },
  "homepage": "https://github.com/Giftbit/internal-edhi#readme",
  "private": true,
  "dependencies": {

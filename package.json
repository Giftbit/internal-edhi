{
  "name": "internal-edhi",
  "version": "1.0.0",
  "description": "Edhi takes care of users.",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf ./dist",
    "lint": "eslint . --ext .ts,.tsx",
    "migrate": "ts-node migrate.ts",
    "migrate:debug": "DEBUG=true npm run migrate",
    "test": "npm run test:startdb; TEST_ENV=true TEST_STRIPE_LOCAL=${TEST_STRIPE_LOCAL:=true} LOG_LEVEL=${LOG_LEVEL:=silent} AWS_REGION=us-west-2 AWS_ACCESS_KEY_ID=AKID AWS_SECRET_ACCESS_KEY=SECRET LIGHTRAIL_DOMAIN=api.lightrail.local LIGHTRAIL_EMAIL_DOMAIN=lightrail.local LIGHTRAIL_WEBAPP_DOMAIN=lightrail.local OBJECT_TABLE=ObjectTable TOKEN_ACTION_TABLE=TokenActionTable mocha --recursive --throw-deprecation --require ts-node/register --require ./src/utils/testUtils/setLogLevel.ts --require ./src/utils/testUtils/dotEnvSafe.ts --require stripe-stateful-mock/autostart --exit \"src/**/*.test.ts\"; npm run test:stopdb",
    "test:debug": "LOG_LEVEL=debug npm run test",
    "test:startdb": "docker start edhi-test || (docker run --name edhi-test -p 8008:8000 amazon/dynamodb-local)",
    "test:stopdb": "docker stop edhi-test",
    "test:stripeLive": "TEST_STRIPE_LOCAL=false npm run test",
    "test:stripeLiveDebug": "TEST_STRIPE_LOCAL=false npm run test:debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giftbit/sam-scaffold.git"
  },
  "keywords": [],
  "author": "",
  "license": "NONE",
  "bugs": {
    "url": "https://github.com/Giftbit/internal-edhi/issues"
  },
  "homepage": "https://github.com/Giftbit/internal-edhi#readme",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@types/aws-lambda": "^8.10.46",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.10",
    "@types/mocha": "^7.0.2",
    "@types/node": "^10.x",
    "@types/sinon": "^7.5.2",
    "@types/stripe": "^7.13.22",
    "@types/superagent": "^3.8.7",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "aws-sdk": "^2.639.0",
    "babel-loader": "^8.0.5",
    "bcryptjs": "^2.4.3",
    "cassava": "^2.5.2",
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.2",
    "dotenv-safe": "^8.2.0",
    "dynameh": "^4.3.0",
    "eslint": "^6.8.0",
    "file-loader": "^5.1.0",
    "giftbit-cassava-routes": "git+ssh://git@github.com/Giftbit/giftbit-cassava-routes.git#semver:^10.4.1",
    "giftbit-lambda-metricslib": "git+ssh://git@github.com/Giftbit/giftbit-lambda-metricslib.git#semver:^3.0.0",
    "json-loader": "^0.5.7",
    "loglevel": "^1.6.7",
    "loglevel-plugin-prefix": "^0.8.4",
    "mocha": "^7.1.0",
    "mysql2": "^2.1.0",
    "otplib": "^12.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "stripe": "^7.15.0",
    "stripe-stateful-mock": "0.0.12",
    "superagent": "^3.8.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "twilio": "^3.40.0",
    "typescript": "^3.8.3",
    "uuid": "^7.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "zip-webpack-plugin": "^3.0.0"
  }
}

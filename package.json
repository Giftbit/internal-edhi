    "@babel/preset-env": "^7.6.0",
    "@types/aws-lambda": "^8.10.31",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.x",
    "@types/sinon": "^7.0.13",
    "@types/stripe": "^6.32.1",
    "@types/superagent": "^4.1.3",
    "@types/uuid": "^3.4.5",
    "aws-sdk": "^2.531.0",
    "babel-loader": "^8.0.5",
